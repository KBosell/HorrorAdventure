<?xml version='1.0' encoding='utf-8'?>
<widget id="com.kelseybosell.horroradventure" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Vibration">
        <param name="android-package" value="org.apache.cordova.vibration.Vibration" />
    </feature>
    <feature name="Insomnia">
        <param name="android-package" value="nl.xservices.plugins.Insomnia" />
    </feature>
    <icon density="ldpi" src="../images/Backgrounds/Splash.png" />
    <icon density="mdpi" src="../images/Backgrounds/Splash.png" />
    <icon density="hdpi" src="../images/Backgrounds/Splash.png" />
    <icon density="xhdpi" src="../images/Backgrounds/Splash.png" />
    <name>Horror Adventure</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <gap:plugin name="org.apache.cordova.vibration" version="0.3.11" />
    <gap:plugin name="de.appplant.cordova.plugin.hidden-statusbar-overlay" />
    <gap:plugin name="nl.x-services.plugins.insomnia" />
    <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" />
    <preference name="SplashScreen" value="../images/Backgrounds/Splash.png" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
